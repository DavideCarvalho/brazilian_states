{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "curly": [true, "as-needed"],
        "arrow-parens": [false, "ban-single-arg-parens"],
        "interface-name": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
