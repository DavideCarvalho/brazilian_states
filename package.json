{
  "name": "br_states",
  "version": "1.8.2",
  "description": "",
  "main": "lib/api/estados.js",
  "author": {
    "name": "Davi de Carvalho",
    "email": "davi_carvalho96@hotmail.com",
    "url": "https://github.com/DavideCarvalho"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DavideCarvalho/brazilian_states"
  },
  "bugs": {
    "url": "https://github.com/DavideCarvalho/brazilian_states/issues"
  },
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "npm run build:types & npm run build:js && copyfiles -f ./lib/src/api/estados.d.ts ./lib/api && copyfiles -f ./lib/src/api/estados/*.d.ts ./lib/api/estados && copyfiles -f ./lib/src/types/*.d.ts ./lib/types && rimraf lib/src && rimraf lib/__tests__",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts\" --source-maps inline",
    "generatedoc": "documentation build src/** -f md -o docs.md"
  },
  "dependencies": {
    "@types/lodash": "^4.14.119",
    "lodash": "^4.17.5",
    "remove-accents": "^0.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/chai": "^4.1.7",
    "@types/jest": "^23.3.12",
    "@types/mocha": "^5.2.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-import": "^1.11.0",
    "benchmark": "^2.1.4",
    "chai": "^4.1.2",
    "copyfiles": "^2.1.0",
    "coveralls": "^3.0.0",
    "documentation": "^6.3.2",
    "jest": "^23.6.0",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3",
    "nyc": "^11.7.1",
    "rimraf": "^2.6.3",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2"
  },
  "nyc": {
    "exclude": [
      "src/api/estados/**.js",
      "test"
    ]
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    }
  }
}
